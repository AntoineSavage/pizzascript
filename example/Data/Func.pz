# Data.Func

(func () '())           # constant function
(func (n) (add n 1))    # unary function
(func (n m) (add n m))  # binary function
# etc
(func args args)        # varargs function, this is similar to how 'list is implemented

# useful functions
(def id (func (x) x))
(def apply (func (f x) (f x)))
(def flip (func (f x y) (f y x)))
(def cmp (func (f g) (func (x) (f (g x)))))
